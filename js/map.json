{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Average estate prices in Australian cities",
  "width": 800,
  "height": 400,
  "projection": {"type": "mercator"},
  "data": {
    "url": "https://raw.githubusercontent.com/BoyangYu1589/FIT3179ASS2/main/js/MB_2016_NT.json",
    "format": {"type": "topojson", "feature": "MB_2016_NT"}
  },
  "transform": [
    {
      "lookup": "properties.SA3_NAME16",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/BoyangYu1589/FIT3179ASS2/main/data/RealEstateAU_1000_cleaned.csv"
        },
        "key": "city",
        "fields": ["price"]
      }
    },
    {"calculate": "datum.price + 0.1", "as": "Average Price"}
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Average Price",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.SA3_NAME16", "type": "nominal", "title": "city"},
      {"field": "Average Price", "type": "quantitative"}  
    ]
  }
}