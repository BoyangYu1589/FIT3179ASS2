{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "description": "A donut chart showing the percentage of different property types with at least ten occurrences.",
    "data": {
      "url": "https://raw.githubusercontent.com/BoyangYu1589/FIT3179ASS2/main/data/RealEstateAU_1000_Samples%20-%20RealEstateAU_1000_Samples.csv"
    },
    "transform": [
      {
        "aggregate": [{"op": "count", "as": "count"}],
        "groupby": ["property_type"]
      },
      {
        "filter": "datum.count >= 10"
      },
      {
        "window": [{"op": "sum", "field": "count"}]
      },
      {
        "calculate": "datum.count / 1000",
        "as": "percentage"
      }
    ],
    "mark": {
      "type": "arc",
      "innerRadius": 50
    },
    "encoding": {
      "theta": {"field": "percentage", "type": "quantitative"},
      "color": {"field": "property_type", "type": "nominal"},
      "tooltip": [
        {"field": "property_type", "type": "nominal", "title": "Property Type"},
        {"field": "percentage", "type": "quantitative", "title": "Percentage", "format": ".1%"}
      ]
    }
  }